<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    android:id="@+id/popup_write_normal"
    >
    <ImageView
        android:id="@+id/iv_popup_write_normal"
        android:layout_width="200dp"
        android:layout_height="300dp"
        android:scaleType="fitXY"
        android:src="@drawable/kacl_table"

        />
    <ImageButton
        android:id="@+id/bt_popup_write_normal"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignEnd="@+id/iv_popup_write_normal"
        android:src="@drawable/btn_write_normal_popup_cancel"
        />
</RelativeLayout>